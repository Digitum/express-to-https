{
  "name": "express-to-https",
  "version": "1.0.0",
  "description": "\"Express middleware to properly redirect http requests to https using the x-forwarded-proto header.\"",
  "main": "lib/index.js",
  "scripts": {
    "build:lib": "./node_modules/.bin/babel src -d lib --ignore *.spec.js",
    "test": "npm run test:mocha",
    "test:mocha": "mocha --require babel-register test/all.test.js",
    "test:watch": "npm run test:mocha -- --watch",
    "prepublish": "npm run build:lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eezing/express-to-https.git"
  },
  "keywords": [
    "express",
    "middleware",
    "https",
    "redirect",
    "forward",
    "proto",
    "header"
  ],
  "author": "Eric Zingeler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eezing/express-to-https/issues"
  },
  "homepage": "https://github.com/eezing/express-to-https#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  }
}
